{
  "name": "next-js-boilerplate",
  "version": "3.0.0",
  "scripts": {
    "start": "next start",
    "dev": "next dev",
    "local": "env-cmd -f .env.local next dev -p 6600",
    "debug": "cross-env NODE_ENV=local next dev",
    "build": "cross-env NODE_ENV=production next build && next export",
    "build:dev": "cross-env NODE_ENV=development next build && next export",
    "build-stats": "cross-env ANALYZE=true npm run build",
    "export": "next export",
    "deploy": "next build && next export && aws s3 sync ./out s3://mementomori.prod",
    "deploy:dev": "next build && next export && aws s3 sync ./out s3://mementomori.prod",
    "clean": "rimraf .next out",
    "lint": "next lint --fix",
    "check-types": "tsc --noEmit --pretty && tsc --project cypress --noEmit --pretty",
    "test": "jest",
    "commit": "cz",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test dev http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 cypress:headless",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.10.14",
    "@mui/x-date-pickers": "^5.0.8",
    "axios": "^1.4.0",
    "dayjs": "^1.11.8",
    "env-cmd": "^10.1.0",
    "framer-motion": "^10.12.16",
    "http-status-codes": "^2.2.0",
    "moment": "^2.29.4",
    "next": "^13.0.0",
    "next-seo": "^5.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-material-ui-carousel": "^3.4.2",
    "react-slick": "^0.29.0",
    "serverless": "^3.25.1",
    "slick-carousel": "^1.8.1",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@commitlint/cz-commitlint": "^17.1.2",
    "@next/bundle-analyzer": "^13.0.0",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.5",
    "@types/react": "^18.0.23",
    "@types/react-slick": "^0.23.10",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "autoprefixer": "^10.4.12",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.13",
    "cypress": "^10.11.0",
    "eslint": "^8.26.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^13.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-tailwindcss": "^3.6.2",
    "eslint-plugin-testing-library": "^5.9.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "sass": "^1.63.3",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.8.4"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "author": "Ixartz (https://github.com/ixartz)"
}
